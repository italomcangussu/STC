{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./*"],
      "@supabase/auth-js": ["./node_modules/@supabase/auth-js/dist/main/index.d.ts"],
      "@supabase/auth-js/*": ["./node_modules/@supabase/auth-js/dist/main/*"],
      "@supabase/functions-js": ["./node_modules/@supabase/functions-js/dist/main/index.d.ts"],
      "@supabase/functions-js/*": ["./node_modules/@supabase/functions-js/dist/main/*"],
      "@supabase/realtime-js": ["./node_modules/@supabase/realtime-js/dist/main/index.d.ts"],
      "@supabase/realtime-js/*": ["./node_modules/@supabase/realtime-js/dist/main/*"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}
